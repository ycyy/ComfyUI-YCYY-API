{
    "YCYY_Gemini_Image_API": {
        "display_name": "Gemini 图像 API",
        "description": "该节点使用 Google Gemini 图像 API 生成或修改图像",
        "inputs": {
            "images": {
                "name": "图像",
                "tooltip": "可选的图像，用作模型的上下文"
            },
            "prompt": {
                "name": "prompt"
            },
            "model": {
                "name": "模型"
            },
            "aspectRatio": {
                "name": "图像宽高比",
                "tooltip": "该模型默认将输出图像大小与输入图像大小匹配，否则生成 1:1 正方形。您可以使用宽高比来控制输出图像的宽高比。"
            },
            "seed": {
                "name": "种子",
                "tooltip": "用于生成噪声的随机种子。"
            }
        },
        "outputs": {
            "0": {
                "name": "图像"
            },
            "1": {
                "name": "字符串"
            }
        }
    },
    "YCYY_Gemini_Image_Preset":{
        "display_name": "Gemini 图像预设",
        "description": "该节点为 Gemini 图像 API 提供预设值",
        "inputs": {
            "preset":{
                "name": "预设名称",
                "tooltip": "预设名称"
            },
            "description":{
                "name": "预设描述",
                "tooltip": "预设描述"
            },
            "prompt":{
                "name": "预设提示词",
                "tooltip": "预设提示词"
            }
        },
        "outputs": {
            "0": {
                "name": "字符串"
            }
        }
    },
    "YCYY_Ollama_VLM_API":{
        "display_name": "Ollama 视觉 API",
        "description": "这个节点使用Ollama VLM 模型进行图片推理分析",
        "inputs": {
            "image":{
                "name": "图像",
                "tooltip": "用于分析的图像"
            },
            "model": {
                "name": "模型"
            }
        },
        "outputs": {
            "0": {
                "name": "字符串"
            }
        }
    },
   "YCYY_Ollama_LLM_API":{
        "display_name": "Ollama 语言 API",
        "description": "该节点使用 Ollama 大语言模型进行对话",
        "inputs": {
            "system_prompt":{
                "name": "system_prompt"
            },
            "user_prompt":{
                "name": "user_prompt"
            },
            "model": {
                "name": "模型"
            },
            "persist_context": {
                "name": "持久化上下文",
                "tooltip": "在调用之间保留聊天上下文（多轮对话）"
            }
        },
        "outputs": {
            "0": {
                "name": "结果",
                "tooltip": "返回结果"
            },
            "1": {
                "name": "上下文",
                "tooltip": "所有历史对话"
            }
        }
    }
}